{
  "extends": [
    "prettier"
  ],
  "plugins": [
    "prettier",
    "react"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true
  },
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-uses-vars": "error"
  }
}
