{
  "extends": [
    "prettier"
  ],
  "plugins": [
    "babel",
    "prettier",
    "react"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-uses-vars": "error"
  }
}
