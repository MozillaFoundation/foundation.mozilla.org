{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "headline": {
        "type": "string"
      },
      "excerpt": {
        "type": "string"
      },
      "outlet": {
        "type": "string"
      },
      "date": {
        "type": "string",
        "format": "date"
      },
      "link": {
        "type": "string"
      },
      "author": {
        "type": ["string", "null"]
      },
      "glyph": {
        "type": ["string", "null"]
      },
      "featured": {
        "type": "boolean"
      }
    },
    "required": [
      "headline",
      "outlet",
      "date",
      "link",
      "featured"
    ]
  }
}
