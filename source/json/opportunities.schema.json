{
  "type": "array",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "",
  "minItems": 1,
  "uniqueItems": true,
  "items": {
    "type": "object",
    "required": [
      "name",
      "description",
      "link",
      "featured",
      "page"
    ],
    "properties": {
      "name": {
        "type": "string"
      },
      "description": {
        "type": "string"
      },
      "link": {
        "type": "object",
        "properties": {
          "slug": {
            "type": "string"
          },
          "label": {
            "type": "string"
          }
        },
        "required": [
          "slug",
          "label"
        ]
      },
      "image": {
        "type": "string"
      },
      "featured": {
        "type": "boolean"
      },
      "page": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "header": {
            "type": "string"
          },
          "content": {
            "type": "string"
          },
          "sign-up": {
            "type": "object",
            "properties": {
              "header": {
                "type": "string"
              },
              "description": {
                "type": "string"
              }
            },
            "required": [
              "header",
              "description"
            ]
          }
        },
        "required": [
          "title",
          "header",
          "content"
        ]
      }
    }
  }
}
